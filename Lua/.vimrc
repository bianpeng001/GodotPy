
set langmenu=en_US
let $LANG = 'en_US'
language en_US.utf8

set number
syntax on

set guifont=Jetbrains\ Mono\ NL:h10

" set guioptions = egmrLtT
set guioptions-=T
set guioptions-=r
set guioptions-=m
set lines=50 columns=120
colorscheme sorbet

" python indent
set backspace=indent,eol,start
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent
" set showmatch

set ruler
set showcmd


